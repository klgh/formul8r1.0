

<div class="jumbotron">
  <div class="container">
    <div class="row-fluid">
      <div class="col-lg-12 centered">

        <%= form_for :login,  :html=>{:class => "form-signin"} do |f| %>
            <%= link_to image_tag("form_log_logo.png"),{ :controller => "users", :action => 'index'}, {:class => ""}  %><br><br>
            <h3>get started today.</h3>
            <p>have an reo photo&trade; account?  <%= link_to "log in", signin_url, :class=>"login-link" %> here.</p>  
            <p>email</p>
            <%=text_field_tag :email, '', :class=>"form-control" %><br>
            <p>password</p>
            <%=password_field_tag :password, '', :class => "form-control" %>
            <p>confirm password</p>
            <%=password_field_tag :password, '', :class => "form-control" %>
            <p class="font9">By clicking this button, you agree to formul8r's <%= link_to("Terms of Use",nil, :class=>"login-link" )%> and <%= link_to("Privacy Policy",nil, :class=>"login-link" )%>.</p>
            <%=submit_tag "create",  :class=> "btn btn-signup" %>
        <% end %>

        <p class="font8 form-signin">&copy;2013, Mobile Business Concepts. all rights reserved. info@moblbc | 678 973 2288</p>
      </div>
    </div>
  </div>
</div>